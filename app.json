{
  "expo": {
    "name": "Veho Go Charge",
    "description":"an app for managing car chargers",
    "slug": "VEHO-GO-CHARGE",
    "platforms":["ios","android","web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier":"com.veho-go.charge"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
